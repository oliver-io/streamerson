{
  "name": "@streamerson/benchmarking",
  "version": "0.0.1",
  "author": "oliver-io <oliver.l.carrillo@gmail.com>",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "start:api": "node ./src/reference/gateway",
    "start:worker": "node ./src/reference/worker",
    "build": "tsc && cp .env ./dist",
    "prebundle": "rm -rf dist && yarn build",
    "bundle": "cd dist/src/reference && yarn --production",
    "pretest": "yarn build",
    "test": "node --test"
  },
  "dependencies": {
    "@streamerson/core": "^0.0.1",
    "@streamerson/gateway-fastify": "^0.0.1",
    "fastify": "^4.23.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.6.4",
    "@types/uuid": "^8.3.4",
    "pino-pretty": "^7.6.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": "xo",
    "overrides": [
      {
        "extends": [
          "xo-typescript"
        ],
        "files": [
          "*.ts",
          "*.tsx"
        ]
      }
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {}
  },
  "prettier": "@jupiterone/typescript-tools/config/prettier"
}
